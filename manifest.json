{
    "manifest_version": 3,
    "name": "Hello world Extension",
    "version": "1.0",
    "description": "Extension that displays hello world.",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icon16.svg",
            "48": "icon48.svg",
            "128": "icon128.svg"
        }
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';"
    }
}